<h1> Total number of people who live in <%= @nation %></h1>
<p> <%= @members.length %></p>
<% @members.each do |member| %>
<dl>
  <dt>Name: <%= member[:name] %></dt>
  <% member[:allies].each do |ally| %>
    <dd id=<%=member[:name] %>-<%= ally %>>Member Allies: <%= ally %></dd>
  <% end %>
  <% member[:enemies].each do |enemy| %>
    <dd id=<%= member[:name]%>-<%= enemy %>>Member Enemies: <%= enemy %></dd>
    <% end %>
  <dd>Affiliation: <%= member[:affiliation] %></dd>
  <dd id=<%= member[:name] %>-img><img alt=<%= member[:name] %> src=<%= member[:photoUrl] %>></dd>
<% end %>
</dl>
